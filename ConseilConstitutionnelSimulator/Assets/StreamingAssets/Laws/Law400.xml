<?xml version="1.0" encoding="utf-8"?>
<Law xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Id="400">
  <Title>Réduction de l'imposition sur le bénéfice</Title>
  <Description>Car nous ne saurions voler le pain à la bouche des actionnaires.</Description>
  <YesPropertyModifiers>
    <PropertyModifier AttributeName="GOVERNMENTOPINION" Value="-4" />
    <PropertyModifier AttributeName="POPULACEOPINION" Value="0" />
    <PropertyModifier AttributeName="ECONOMY" Value="6" />
    <PropertyModifier AttributeName="EMPLOYMENT" Value="0" />
    <PropertyModifier AttributeName="RELIGION" Value="0" />
    <PropertyModifier AttributeName="PERSONALMONEY" Value="15000" />
  </YesPropertyModifiers>
  <YesLawsToAdd>
    <LawId>67</LawId>
  </YesLawsToAdd>
  <NoPropertyModifiers>
    <PropertyModifier AttributeName="GOVERNMENTOPINION" Value="3" />
    <PropertyModifier AttributeName="POPULACEOPINION" Value="-6" />
    <PropertyModifier AttributeName="ECONOMY" Value="0" />
    <PropertyModifier AttributeName="EMPLOYMENT" Value="-7" />
    <PropertyModifier AttributeName="RELIGION" Value="0" />
    <PropertyModifier AttributeName="PERSONALMONEY" Value="0" />
  </NoPropertyModifiers>
  <NoLawsToAdd />
  <MinimizePropertyModifiers />
  <MinimizeLawsToAdd>
    <LawId>-1</LawId>
  </MinimizeLawsToAdd>
  <MaximizePropertyModifiers />
  <MaximizeLawsToAdd>
    <LawId>-1</LawId>
  </MaximizeLawsToAdd>
</Law>