<?xml version="1.0" encoding="utf-8"?>
<Law xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Id="461">
  <Title>Limitation des aides à la création d'entreprise</Title>
  <Description>Délègue l'investissement une partie de l'investissement aux acteurs économiques privés : mauvais pour l'économie mais bon pour nos dépenses !</Description>
  <YesPropertyModifiers>
    <PropertyModifier AttributeName="GOVERNMENTOPINION" Value="10" />
    <PropertyModifier AttributeName="POPULACEOPINION" Value="-7" />
    <PropertyModifier AttributeName="ECONOMY" Value="-13" />
    <PropertyModifier AttributeName="EMPLOYMENT" Value="-7" />
    <PropertyModifier AttributeName="RELIGION" Value="0" />
    <PropertyModifier AttributeName="PERSONALMONEY" Value="3000" />
  </YesPropertyModifiers>
  <YesLawsToAdd>
    <LawId>128</LawId>
  </YesLawsToAdd>
  <NoPropertyModifiers>
    <PropertyModifier AttributeName="GOVERNMENTOPINION" Value="-10" />
    <PropertyModifier AttributeName="POPULACEOPINION" Value="0" />
    <PropertyModifier AttributeName="ECONOMY" Value="13" />
    <PropertyModifier AttributeName="EMPLOYMENT" Value="6" />
    <PropertyModifier AttributeName="RELIGION" Value="0" />
    <PropertyModifier AttributeName="PERSONALMONEY" Value="15000" />
  </NoPropertyModifiers>
  <NoLawsToAdd />
  <MinimizePropertyModifiers />
  <MinimizeLawsToAdd>
    <LawId>-1</LawId>
  </MinimizeLawsToAdd>
  <MaximizePropertyModifiers />
  <MaximizeLawsToAdd>
    <LawId>-1</LawId>
  </MaximizeLawsToAdd>
</Law>