    <PropertyModifier AttributeName="EMPLOYMENT" Value="6" />
    <PropertyModifier AttributeName="RELIGION" Value="0" />
    <PropertyModifier AttributeName="PERSONALMONEY" Value="3000" />
  </YesPropertyModifiers>
